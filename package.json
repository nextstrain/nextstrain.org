    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^1.3.1",
    "jest": "^26.4.2",
    "jest-extended": "^1.1.0",
    "start-server-and-test": "^1.11.4"
  },
  "cacheDirectories": [
    "node_modules",
    "static-site/node_modules"
