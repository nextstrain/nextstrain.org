{
  "name": "static",
  "description": "Static content for nextstrain.org",
  "version": "1.0.1",
  "author": "james hadfield",
  "keywords": [
    "nextstrain",
    "bioinformatics",
    "phylodynamics"
  ],
  "license": "MIT",
  "main": "n/a",
  "engines": {
    "node": "9.6.x",
    "npm": "5.6.x"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "test": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "heroku-postbuild": "gatsby build"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "gatsby": "^1.9.256",
    "gatsby-link": "^1.6.7",
    "gatsby-plugin-catch-links": "^1.0.4",
    "gatsby-plugin-feed": "^1.3.4",
    "gatsby-plugin-google-analytics": "^1.0.3",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^1.0.3",
    "gatsby-plugin-nprogress": "^1.0.2",
    "gatsby-plugin-offline": "^1.0.3",
    "gatsby-plugin-react-helmet": "^1.0.2",
    "gatsby-plugin-sharp": "1.6.24",
    "gatsby-plugin-sitemap": "^1.2.1",
    "gatsby-plugin-styled-components": "^2.0.2",
    "gatsby-plugin-twitter": "^1.0.6",
    "gatsby-remark-autolink-headers": "^1.4.3",
    "gatsby-remark-copy-linked-files": "^1.5.1",
    "gatsby-remark-images": "^1.5.3",
    "gatsby-remark-prismjs": "^1.2.0",
    "gatsby-remark-responsive-iframe": "^1.4.2",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "^1.4.2",
    "gatsby-transformer-remark": "^1.7.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.3.3",
    "react-helmet": "^5.1.3",
    "react-icons": "^2.2.7",
    "react-scrollable-anchor": "^0.6.1",
    "react-share": "^1.15.1",
    "react-tweet-embed": "^1.1.0",
    "react-twitter-widgets": "^1.4.0",
    "save": "^2.3.2",
    "styled-components": "^2.2.3"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.3",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "write-good": "^0.11.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
